+
+        return user;
+      } catch (err) {
+        setError(err as AuthError);
+      } finally {
+        setLoading(false);
+      }
+    },
+    [auth, createProvider]
+  );
+
+  return [doSignInWithPopup, loggedInUser, loading, error];
+};
